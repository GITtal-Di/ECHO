{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "^/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "^/health",
            "dest": "/api/index"
        },
        {
            "src": "^/$",
            "dest": "/api/index"
        }
    ]
}